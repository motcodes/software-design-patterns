#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/matthiasoberholzer/Documents/code/software-design-patterns/node_modules/.pnpm/esbuild@0.13.15/node_modules/esbuild/bin/node_modules:/Users/matthiasoberholzer/Documents/code/software-design-patterns/node_modules/.pnpm/esbuild@0.13.15/node_modules/esbuild/node_modules:/Users/matthiasoberholzer/Documents/code/software-design-patterns/node_modules/.pnpm/esbuild@0.13.15/node_modules:/Users/matthiasoberholzer/Documents/code/software-design-patterns/node_modules/.pnpm/node_modules:/Users/matthiasoberholzer/Documents/code/software-design-patterns/node_modules:/Users/matthiasoberholzer/Documents/code/node_modules:/Users/matthiasoberholzer/Documents/node_modules:/Users/matthiasoberholzer/node_modules:/Users/node_modules:/node_modules:/Users/matthiasoberholzer/Documents/code/software-design-patterns/node_modules/.pnpm/vite@2.7.1/node_modules/vite/node_modules:/Users/matthiasoberholzer/Documents/code/software-design-patterns/node_modules/.pnpm/vite@2.7.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/matthiasoberholzer/Documents/code/software-design-patterns/node_modules/.pnpm/esbuild@0.13.15/node_modules/esbuild/bin/node_modules:/Users/matthiasoberholzer/Documents/code/software-design-patterns/node_modules/.pnpm/esbuild@0.13.15/node_modules/esbuild/node_modules:/Users/matthiasoberholzer/Documents/code/software-design-patterns/node_modules/.pnpm/esbuild@0.13.15/node_modules:/Users/matthiasoberholzer/Documents/code/software-design-patterns/node_modules/.pnpm/node_modules:/Users/matthiasoberholzer/Documents/code/software-design-patterns/node_modules:/Users/matthiasoberholzer/Documents/code/node_modules:/Users/matthiasoberholzer/Documents/node_modules:/Users/matthiasoberholzer/node_modules:/Users/node_modules:/node_modules:/Users/matthiasoberholzer/Documents/code/software-design-patterns/node_modules/.pnpm/vite@2.7.1/node_modules/vite/node_modules:/Users/matthiasoberholzer/Documents/code/software-design-patterns/node_modules/.pnpm/vite@2.7.1/node_modules"
